@article{ WOS:000515493800001,
Author = {Osdaghi, Ebrahim and Young, Anthony J. and Harveson, Robert M.},
Title = {Bacterial wilt of dry beans caused by Curtobacterium flaccumfaciens pv.
   flaccumfaciens: A new threat from an old enemy},
Journal = {MOLECULAR PLANT PATHOLOGY},
Year = {2020},
Volume = {21},
